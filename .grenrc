{
    "dataSource": "prs",
    "prefix": "",
    "onlyMilestones": false,
    "changelogFilename": "CHANGELOG.md",
    "ignoreIssuesWith": [
        "duplicate",
        "wontfix",
        "invalid",
        "help wanted",
        "question",
        "testing"
    ],
    "template": {
        "issue": "- [{{text}}]({{url}}) {{name}}"
    },
    "groupBy": {
        "Breaking changes": ["breaking/api", "breaking"],
        "Enhancements": ["enhancement", "feature"],
        "Bug Fixes": ["bug"],
        "Documentation": ["documentation"],
        "Styling": ["styling"],
        "Dependencies": ["dependencies"],
        "Other": ["chore", "refactor"]
    }
}
